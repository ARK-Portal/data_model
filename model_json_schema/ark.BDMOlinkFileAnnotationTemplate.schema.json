{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example",
  "title": "example",
  "type": "object",
  "properties": {
    "project": {
      "enum": [
        "RA",
        "AIM for RA",
        "SLE",
        "STAMP",
        "ELLIPSS",
        "LOCKIT",
        "V-CoRT"
      ]
    },
    "targetPanelSynID": {
      "not": {
        "type": "null"
      },
      "minLength": 1
    },
    "resourceType": {
      "enum": [
        "code",
        "metadata",
        "experimental data",
        "figure"
      ]
    },
    "targetPanel": {
      "not": {
        "type": "null"
      },
      "minLength": 1
    },
    "visitID": {},
    "specimenModality": {
      "enum": [
        "single specimen",
        "multispecimen"
      ]
    },
    "fileFormat": {
      "enum": [
        "docx",
        "xls",
        "bam",
        "tgz",
        "fcs",
        "fam",
        "bim",
        "bai",
        "erate",
        "bed",
        "h5",
        "h5ad",
        "tbi",
        "fastq",
        "rds",
        "txt",
        "parquet",
        "vcf",
        "geojson",
        "zip",
        "rec",
        "xlsx",
        "csv",
        "mtx",
        "tar.gz",
        "pdf",
        "tsv",
        "dose",
        "info"
      ]
    },
    "diagnosis": {
      "enum": [
        "RA",
        "lupus nephritis",
        "psoriasis",
        "cutaneous lupus erythematosus",
        "dermatomyositis",
        "Sjogren's disease",
        "SLE",
        "scleroderma",
        "psoriatic arthritis",
        "control",
        "OA",
        "At-Risk RA",
        "vitiligo"
      ]
    },
    "Component": {
      "not": {
        "type": "null"
      },
      "minLength": 1
    },
    "targetPanelSize": {},
    "plateID": {
      "not": {
        "type": "null"
      },
      "minLength": 1
    },
    "biospecimenType": {
      "type": "array",
      "items": {
        "enum": [
          "uvea",
          "saliva",
          "total leukocytes",
          "suction blister fluid",
          "stool",
          "skin swab",
          "synovial fluid",
          "none",
          "salivary gland",
          "kidney biopsy",
          "skin biopsy",
          "whole blood",
          "plasma",
          "fibroblast-like synoviocyte",
          "serum",
          "PBMCs",
          "urine",
          "suction blister cells",
          "synovial tissue"
        ]
      },
      "maxItems": 19
    },
    "Filename": {
      "not": {
        "type": "null"
      },
      "minLength": 1
    },
    "dataType": {
      "type": "array",
      "items": {
        "enum": [
          "epigenomics",
          "genomics",
          "cytometry",
          "proteomics",
          "transcriptomics",
          "multimodal",
          "immune repertoire profiling",
          "metabolomics",
          "lipidomics",
          "immunostaining",
          "histology",
          "microbiome"
        ]
      },
      "maxItems": 12
    },
    "dataSubtype": {
      "type": "array",
      "items": {
        "enum": [
          "spatial",
          "pseudobulk",
          "none",
          "single-nucleus",
          "single-cell",
          "bulk"
        ]
      },
      "maxItems": 6
    },
    "platform": {
      "type": "array",
      "items": {
        "enum": [
          "Illumina NextSeq 500",
          "Illumina NovaSeq 6000",
          "Fluidigm BioMark",
          "Olink Signature Q100",
          ""
        ]
      },
      "maxItems": 4
    },
    "program": {
      "enum": [
        "Community Contribution",
        "AMP AIM",
        "AMP RA/SLE"
      ]
    }
  },
  "required": [
    "project",
    "targetPanelSynID",
    "resourceType",
    "targetPanel",
    "specimenModality",
    "fileFormat",
    "diagnosis",
    "Component",
    "plateID",
    "biospecimenType",
    "Filename",
    "dataType",
    "dataSubtype",
    "program"
  ]
}