{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/ark",
  "title": "ark",
  "type": "object",
  "properties": {
    "Component": {
      "not": {
        "type": "null"
      },
      "minLength": 1
    },
    "nucleicAcidSource": {
      "type": "array",
      "items": {
        "enum": [
          "multiplexing oligo",
          "globin-depleted RNA",
          "BCR mRNA",
          "CRISPR protospacer feature barcode",
          "intracellular protein feature barcode",
          "TCR mRNA",
          "surface protein feature barcode",
          "Tn5-accessible gDNA",
          "poly(A) RNA",
          "antigen capture barcode",
          "rRNA-depleted RNA",
          "gDNA"
        ]
      },
      "maxItems": 12
    },
    "specimenModality": {
      "enum": [
        "multispecimen",
        "single specimen"
      ]
    },
    "totalReads": {
      "not": {
        "type": "null"
      },
      "minLength": 1
    },
    "softwareAndVersion": {
      "enum": [
        "Cell Ranger v3.0.0",
        "Cell Ranger v6.1.1",
        "Space Ranger 3.0.1",
