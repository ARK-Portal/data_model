{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/ark",
  "title": "ark",
  "type": "object",
  "properties": {
    "specimenModality": {
      "enum": [
        "multispecimen",
        "single specimen"
      ]
    },
    "softwareAndVersion": {
      "enum": [
        "Space Ranger 3.1.1",
        "Cell Ranger v8.0.0",
        "Cell Ranger v7.1.0",
        "Cell Ranger v5.0.0",
        "Cell Ranger v4.0.0",
        "Cell Ranger v3.0.0",
        "Cell Ranger v5.0.1",
        "Space Ranger 3.0.0",
        "Cell Ranger v7.0.1",
        "Cell Ranger v7.2.0",
        "Cell Ranger v6.1.2",
        "demuxlet",
        "Cell Ranger v6.0.0",
        "Cell Ranger v3.0.2",
        "Space Ranger 3.0.1",
        "Space Ranger 3.1.2",
        "Space Ranger 3.1.3",
        "Cell Ranger v8.0.1",
        "Cell Ranger v3.1.0",
        "Cell Ranger v6.1.1",
        "Cell Ranger ATAC v1.1.0",
        "Cell Ranger v6.1.0",
        "BD FACSDiva 8.0.1",
        "Cell Ranger v3.0.1",
        "Cell Ranger v9.0.0",
        "Cell Ranger v6.0.1",
        "Cell Ranger 9.0.1",
        "Space Ranger 3.1.0",
        "Cell Ranger v6.0.2",
        "Cell Ranger v7.0.0",
        ""
      ]
    },
    "sampleProcessingBatch": {},
    "dataCollectionBatch": {},
    "Component": {
      "not": {
        "type": "null"
      },
      "minLength": 1
    },
    "nucleicAcidSource": {
      "type": "array",
      "items": {
        "enum": [
          "BCR mRNA",
          "antigen capture barcode",
          "poly(A) RNA",
          "gDNA",
          "multiplexing oligo",
          "CRISPR protospacer feature barcode",
          "Tn5-accessible gDNA",
          "surface protein feature barcode",
          "rRNA-depleted RNA",
          "TCR mRNA",
          "intracellular protein feature barcode",
          "globin-depleted RNA"
        ]
      },
      "maxItems": 12
    },
    "platform": {
      "type": "array",
      "items": {
        "enum": [
          "Illumina NextSeq 500",
          "BD LSRFortessa",
          "Hyperion",
          "Visium CytAssist",
          "BD FACSMelody",
          "Not Applicable",
          "Xenium",
          "Chromium Next GEM Chip Q",
          "Chromium GEM-X Single Cell 3' Chip v4",
          "BD FACSLyric Clinical",
          "BD FACSDiscover A8",
          "none",
          "Illumina NovaSeq 6000",
          "Chromium iX",
          "GEM-X OCM 5' Chip",
          "Thermo Fisher Attune CytPix",
          "Illumina HiSeq X Ten",
          "Chromium Xo",
          "Cytek Aurora",
          "Chromium Next GEM Chip G",
          "Cytek Aurora Evo",
          "BD FACSAria III",
          "Thermo Fisher Attune Xenith",
          "Chromium X",
          "Illumina HiSeq 2500",
          "Helios Mass Cytometer",
          "Chromium Controller",
          "Thermo Fisher Attune NxT",
          "BD FACSAria Fusion cell sorter",
          "GEM-X Flex Gene Expression Chip",
          "Illumina NovaSeq X",
          "Chromium Next GEM Chip K",
          "BD FACSCanto II",
          "Sony MA900",
          "BD FACSCanto",
          "BD FACSymphony S6",
          "unknown",
          "BD FACSDiscover S8",
          "Chromium Next GEM Chip H",
          "Chromium Next GEM Chip M",
          "Olink Signature Q100",
          "Fluidigm BioMark",
          "CyTOF XT"
        ]
      },
      "maxItems": 43
    },
    "alignmentReference": {
      "enum": [
        "unknown",
        "10x Cell Ranger Human GRCh38 2024-A",
        "10x Cell Ranger Human GRCh38 2020-A",
        "vdj_GRCh38_alts_ensembl-4.0.0",
        "modified GRCh38"
      ]
    },
    "assay": {
      "type": "array",
      "items": {
        "enum": [
          "SomaScan",
          "ASAPSeq",
          "WGS",
          "scRNASeq",
          "serial IHC",
          "VDJSeq",
          "snRNASeq",
          "H&E",
          "Xenium",
          "GenePS SeqFISH",
          "imaging mass spectrometry",
          "CosMX",
          "scVDJSeq",
          "snATACSeq",
          "Olink Flex",
          "flow cytometry",
          "CITESeq",
          "feature barcode sequencing",
          "WES",
          "Olink Focus",
          "Olink Target 96",
          "imaging mass cytometry",
          "RNASeq",
          "CyTOF",
          "CE-MS",
