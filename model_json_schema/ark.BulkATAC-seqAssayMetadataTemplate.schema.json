{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/ark",
  "title": "ark",
  "type": "object",
  "properties": {
    "specimenModality": {
      "enum": [
        "single specimen",
        "multispecimen"
      ]
    },
    "alignmentReference": {
      "enum": [
        "10x Cell Ranger Human GRCh38 2020-A",
        "vdj_GRCh38_alts_ensembl-4.0.0",
        "10x Cell Ranger Human GRCh38 2024-A",
        "unknown"
      ]
    },
    "softwareAndVersion": {
      "enum": [
        "Space Ranger 3.1.2",
        "Space Ranger 3.0.0",
        "Cell Ranger v3.1.0",
        "Cell Ranger v6.1.2",
        "Cell Ranger v7.1.0",
        "Cell Ranger v5.0.0",
        "demuxlet",
        "Cell Ranger v6.1.1",
        "Cell Ranger v6.1.0",
        "Cell Ranger v3.0.2",
        "Cell Ranger v6.0.0",
        "Cell Ranger v7.0.0",
        "Cell Ranger v7.0.1",
        "Cell Ranger v3.0.1",
        "Space Ranger 3.1.1",
        "Space Ranger 3.0.1",
        "Space Ranger 3.1.0",
        "Cell Ranger v3.0.0",
        "Cell Ranger v6.0.2",
        "Cell Ranger v5.0.1",
        "Cell Ranger v6.0.1",
        "Cell Ranger 9.0.1",
        "Cell Ranger v7.2.0",
        "Cell Ranger v9.0.0",
        "Cell Ranger v8.0.1",
        "Cell Ranger v8.0.0",
        "Space Ranger 3.1.3",
        "BD FACSDiva 8.0.1",
        "Cell Ranger ATAC v1.1.0",
        "Cell Ranger v4.0.0",
        ""
      ]
    },
    "totalReads": {
      "not": {
        "type": "null"
      },
      "minLength": 1
    },
    "sampleProcessingBatch": {},
    "nucleicAcidSource": {
      "type": "array",
      "items": {
        "enum": [
          "surface protein feature barcode",
          "poly(A) RNA",
          "gDNA",
          "antigen capture barcode",
          "BCR mRNA",
          "CRISPR protospacer feature barcode",
          "intracellular protein feature barcode",
          "Tn5-accessible gDNA",
          "TCR mRNA",
          "globin-depleted RNA",
          "rRNA-depleted RNA",
          "multiplexing oligo"
        ]
      },
      "maxItems": 12
    },
    "assay": {
      "type": "array",
      "items": {
        "enum": [
          "Olink Target 96",
          "CE-MS",
          "H&E",
          "scRNASeq",
          "kiloplex",
          "CyTOF",
          "WGS",
          "snATACSeq",
          "serial IHC",
          "CITESeq",
          "WES",
          "CosMX",
          "Xenium",
          "ASAPSeq",
          "snRNASeq",
          "scVDJSeq",
          "feature barcode sequencing",
          "Olink Focus",
          "LC-MS/MS",
          "SNP array",
          "flow cytometry",
          "Olink Target 48",
          "imaging mass spectrometry",
          "VDJSeq",
          "imaging mass cytometry",
          "Olink Reveal",
          "SomaScan",
          "Olink Explore HT",
          "Olink Flex",
          "NULISA",
          "multiplexed ELISA",
          "RNASeq"
        ]
      },
      "maxItems": 32
    },
    "dataCollectionBatch": {},
    "Component": {
      "not": {
        "type": "null"
      },
      "minLength": 1
    },
    "platform": {
      "type": "array",
      "items": {
        "enum": [
          "Not Applicable",
          "BD FACSymphony S6",
          "Helios Mass Cytometer",
          "Thermo Fisher Attune CytPix",
          "Illumina NovaSeq 6000",
          "Thermo Fisher Attune NxT",
          "BD FACSLyric Clinical",
          "GEM-X Flex Gene Expression Chip",
          "Chromium Next GEM Chip K",
          "Xenium",
          "Sony MA900",
          "BD FACSCanto",
          "unknown",
          "Chromium Next GEM Chip M",
          "Illumina HiSeq 2500",
          "Chromium Next GEM Chip H",
          "Hyperion",
          "Olink Signature Q100",
          "Fluidigm BioMark",
          "GEM-X OCM 5' Chip",
          "Chromium iX",
          "Cytek Aurora Evo",
          "BD FACSDiscover A8",
          "Illumina NovaSeq X",
          "Chromium GEM-X Single Cell 3' Chip v4",
          "BD FACSCanto II",
          "Illumina NextSeq 500",
          "BD FACSMelody",
          "none",
          "Chromium Controller",
          "Visium CytAssist",
          "Chromium Xo",
          "BD FACSAria Fusion cell sorter",
          "Chromium Next GEM Chip G",
          "Cytek Aurora",
          "Illumina HiSeq X Ten",
          "Thermo Fisher Attune Xenith",
          "Chromium X",
          "BD LSRFortessa",
          "CyTOF XT",
          "Chromium Next GEM Chip Q",
          "BD FACSDiscover S8",
          "BD FACSAria III"
        ]
      },
      "maxItems": 43
    },
    "libraryPrepMethod": {
      "enum": [
        "Chromium Next GEM Single Cell ATAC v1.1",
        "SMARTer Stranded Total RNA v2",
        "10x Chromium GEM-X Single Cell 3' v4",
        "Takara Human TCR profiling for Illumina",
        "Takara Human scTCR profiling for Illumina",
        "10x GEM-X Universal 5? Gene Expression v3",
        "custom DASH-treatment",
        "Takara Human BCR profiling for Illumina",
        "TruSeq Stranded mRNA",
        "10x Chromium Single Cell Human BCR",
        "10x Chromium Next GEM Single Cell 5' v2",
        "QIAseq miRNA Library",
        "10x Chromium Fixed RNA Human Transcriptome",
        "10x Chromium Next GEM Single Cell ATAC v2",
        "SMART-Seq Human BCR with UMI",
        "10x GEM-X Flex Gene Expression Human",
        "Fluidigm C1 HT",
        "NEBNext Ultra II Directional RNA Library",
        "Nextera XT",
        "Takara Human TCRv2 profiling for Illumina",
        "10x Chromium Single Cell Human TCR",
        "10x Chromium Next GEM Single Cell 3'",
        "NEBNext Human Immune Sequencing Kit",
        "10x Chromium GEM-X Single Cell 5' v3",
        "SMART-Seq Human TCR with UMI",
        "10x Chromium Next GEM Single Cell 5' v1.1",
        "SMART-Seq v4 Ultra Low Input RNA",
        "in-house library prep",
        "10x Chromium Next GEM Single Cell 3' 3.1",
        "CEL-Seq2",
        "Nextera XT DNA"
      ]
    },
    "biospecimenID": {},
    "libraryID": {},
    "10xProbeSetReference": {
      "enum": [
        "Flex Human Transcriptome Probe Set v1.1.0",
        "Visium Human Transcriptome v2",
        "Visium Human Transcriptome v1",
        "Flex Human Transcriptome Probe Set v1.0.1",
        "custom probe set",
        ""
      ]
    }
  },
  "required": [
    "specimenModality",
    "alignmentReference",
    "totalReads",
    "nucleicAcidSource",
    "assay",
    "Component",
    "platform",
    "libraryPrepMethod"
  ],
  "allOf": [
    {
      "if": {
        "properties": {
          "specimenModality": {
            "enum": [
              "single specimen"
            ]
          }
        },
        "required": [
          "specimenModality"
        ]
      },
      "then": {
        "properties": {
          "biospecimenID": {
            "not": {
              "type": "null"
            },
            "minLength": 1
          }
        },
        "required": [
          "biospecimenID"
        ]
      }
    },
    {
      "if": {
        "properties": {
          "specimenModality": {
            "enum": [
              "multispecimen"
            ]
          }
        },
        "required": [
          "specimenModality"
        ]
      },
      "then": {
        "properties": {
          "libraryID": {}
        },
        "required": [
          "libraryID"
        ]
      }
    },
    {
      "if": {
        "properties": {
          "libraryPrepMethod": {
            "enum": [
              "10x GEM-X Flex Gene Expression Human"
            ]
          }
        },
        "required": [
          "libraryPrepMethod"
        ]
      },
      "then": {
        "properties": {
          "10xProbeSetReference": {
            "enum": [
              "Flex Human Transcriptome Probe Set v1.1.0",
              "Visium Human Transcriptome v2",
              "Visium Human Transcriptome v1",
              "Flex Human Transcriptome Probe Set v1.0.1",
              "custom probe set"
            ]
          }
        },
        "required": [
          "10xProbeSetReference"
        ]
      }
    }
  ]
}