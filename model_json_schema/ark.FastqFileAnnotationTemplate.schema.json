{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example",
  "title": "example",
  "type": "object",
  "properties": {
    "fileFormat": {
      "enum": [
        "fastq",
        ""
      ]
    },
    "Filename": {
      "not": {
        "type": "null"
      },
      "minLength": 1
    },
    "resourceType": {
      "enum": [
        "experimental data",
        ""
      ]
    },
    "specimenModality": {
      "enum": [
        "multispecimen",
        "single specimen"
      ]
    },
    "readLength": {},
    "assay": {
      "type": "array",
      "items": {
        "enum": [
          "WES",
          "LC-MS/MS",
          "SomaScan",
          "RNASeq",
          "serial IHC",
          "Olink Explore HT",
          "imaging mass spectrometry",
          "snATACSeq",
          "ASAPSeq",
          "scVDJSeq",
          "NULISA",
          "snRNASeq",
          "WGS",
          "flow cytometry",
          "H&E",
          "CyTOF",
          "Xenium",
          "CosMX",
          "multiplexed ELISA",
          "imaging mass cytometry",
          "VDJSeq",
          "SNP array",
          "feature barcode sequencing",
          "scRNASeq",
          "CE-MS",
          "CITESeq"
        ]
      },
      "maxItems": 26
    },
    "Component": {
      "not": {
        "type": "null"
      },
      "minLength": 1
    },
    "libraryID": {},
    "individualID": {},
    "biospecimenID": {},
    "targetPanel": {},
    "targetPanelSynID": {}
  },
  "required": [
    "Filename",
    "specimenModality",
    "assay",
    "Component"
  ],
  "allOf": [
    {
      "if": {
        "properties": {
          "specimenModality": {
            "enum": [
              "multispecimen"
            ]
          }
        },
        "required": [
          "specimenModality"
        ]
      },
      "then": {
        "properties": {
          "libraryID": {}
        },
        "required": [
          "libraryID"
        ]
      }
    },
    {
      "if": {
        "properties": {
          "specimenModality": {
            "enum": [
              "single specimen"
            ]
          }
        },
        "required": [
          "specimenModality"
        ]
      },
      "then": {
        "properties": {
          "individualID": {
            "not": {
              "type": "null"
            },
            "minLength": 1
          }
        },
        "required": [
          "individualID"
        ]
      }
    },
    {
      "if": {
        "properties": {
          "specimenModality": {
            "enum": [
              "single specimen"
            ]
          }
        },
        "required": [
          "specimenModality"
        ]
      },
      "then": {
        "properties": {
          "biospecimenID": {
            "not": {
              "type": "null"
            },
            "minLength": 1
          }
        },
        "required": [
          "biospecimenID"
        ]
      }
    },
    {
      "if": {
        "properties": {
          "assay": {
            "enum": [
              "feature barcode sequencing"
            ]
          }
        },
        "required": [
          "assay"
        ]
      },
      "then": {
        "properties": {
          "targetPanel": {
            "not": {
              "type": "null"
            },
            "minLength": 1
          }
        },
        "required": [
          "targetPanel"
        ]
      }
    },
    {
      "if": {
        "properties": {
          "assay": {
            "enum": [
              "feature barcode sequencing"
            ]
          }
        },
        "required": [
          "assay"
        ]
      },
      "then": {
        "properties": {
          "targetPanelSynID": {
            "not": {
              "type": "null"
            },
            "minLength": 1
          }
        },
        "required": [
          "targetPanelSynID"
        ]
      }
    }
  ]
}