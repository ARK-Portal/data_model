          "specimenModality"
        ]
      },
      "then": {
        "properties": {
          "libraryID": {
            "not": {
              "type": "null"
            },
            "minLength": 1
          }
        },
        "required": [
          "libraryID"
        ]
      }
    },
    {
      "if": {
        "properties": {
          "libraryPrepMethod": {
            "enum": [
              "10x GEM-X Flex Gene Expression Human"
            ]
          }
