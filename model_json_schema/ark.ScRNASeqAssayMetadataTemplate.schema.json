          "Fluidigm BioMark",
          "BD LSRFortessa",
          "Xenium"
        ]
      },
      "maxItems": 43
    },
    "inputCellCount": {
      "not": {
        "type": "null"
      },
      "minLength": 1
    },
    "percentCellViability": {
      "not": {
        "type": "null"
      },
      "minLength": 1
    },
    "assay": {
      "type": "array",
      "items": {
        "enum": [
          "scRNASeq",
          "NULISA",
