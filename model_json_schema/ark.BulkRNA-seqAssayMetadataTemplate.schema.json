          "NULISA",
          "CE-MS",
          "CITESeq",
          "Olink Target 96",
          "snRNASeq",
          "snATACSeq",
          "Olink Explore HT",
          "Olink Focus",
          "kiloplex"
        ]
      },
      "maxItems": 32
    },
    "Component": {
      "not": {
        "type": "null"
      },
      "minLength": 1
    },
    "platform": {
      "type": "array",
      "items": {
        "enum": [
          "Chromium Next GEM Chip M",
          "Thermo Fisher Attune CytPix",
          "BD FACSDiscover A8",
          "Illumina NextSeq 500",
          "GEM-X Flex Gene Expression Chip",
