{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/ark",
  "title": "ark",
  "type": "object",
  "properties": {
    "specimenModality": {
      "enum": [
        "multispecimen",
        "single specimen"
      ]
    },
    "dataCollectionBatch": {},
    "libraryPrepMethod": {
      "enum": [
