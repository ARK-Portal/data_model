  "required": [
    "biospecimenType",
    "assay",
    "datasetDescription",
    "ARKRelease",
    "diagnosis",
    "dataType",
    "dataSubtype",
    "species",
    "program",
    "datasetType",
    "datasetStatus",
    "project",
    "acknowledgmentStatement",
    "Component"
  ],
  "allOf": [
    {
      "if": {
        "properties": {
          "program": {
            "enum": [
              "AMP RA/SLE"
            ]
          }
        },
        "required": [
          "program"
        ]
      },
      "then": {
        "properties": {
          "programPhase": {
            "type": "array",
            "items": {
              "enum": [
                "II",
                "I"
              ]
            },
            "maxItems": 2
          }
        },
        "required": [
          "programPhase"
        ]
      }
    }
  ]
