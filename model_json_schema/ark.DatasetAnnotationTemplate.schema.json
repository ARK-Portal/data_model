    "datasetType",
    "project",
    "dataType",
    "program",
    "ARKRelease",
    "assay",
    "biospecimenType",
    "acknowledgmentStatement",
    "Component",
    "datasetDescription",
    "diagnosis",
    "dataSubtype",
    "datasetStatus"
