    },
    "project": {
      "enum": [
        "RA",
        "SLE",
        "V-CoRT",
        "STAMP",
        "AIM for RA",
        "LOCKIT",
        "ELLIPSS"
      ]
    },
    "ARKRelease": {
      "enum": [
        "2025.10.R1",
        "2.0",
        "2025.07.R1",
        "2025.12.R1",
        "2025.09.R1",
        "1.0",
        "2025.05.R1",
        "2025.01.R1",
        "2024.08.R1",
        "2025.11.R1",
        "2025.08.R1",
        "2025.06.R1",
        "2024.07.R1",
        "2024.09.R1",
        "2025.04.R1",
        "2024.06.R1",
        "2024.10.R1",
        "2025.02.R1",
        "2024.12.R1",
        "2025.03.R1"
