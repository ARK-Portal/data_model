  "required": [
    "datasetStatus",
    "dataType",
    "project",
    "ARKRelease",
    "biospecimenType",
    "Component",
    "datasetDescription",
    "assay",
    "datasetType",
    "diagnosis",
    "acknowledgmentStatement",
    "program",
    "dataSubtype"
  ],
  "allOf": [
    {
      "if": {
        "properties": {
          "program": {
            "enum": [
              "AMP RA/SLE"
            ]
          }
        },
        "required": [
          "program"
        ]
      },
      "then": {
        "properties": {
          "programPhase": {
            "type": "array",
            "items": {
              "enum": [
                "I",
                "II"
              ]
            },
            "maxItems": 2
          }
        },
        "required": [
          "programPhase"
        ]
      }
    }
  ]
}