  "allOf": [
    {
      "if": {
        "properties": {
          "program": {
            "enum": [
              "AMP RA/SLE"
            ]
          }
        },
        "required": [
          "program"
        ]
      },
      "then": {
        "properties": {
          "programPhase": {
            "type": "array",
            "items": {
              "enum": [
                "I",
                "II"
              ]
            },
            "maxItems": 2
          }
        },
        "required": [
          "programPhase"
        ]
      }
    }
