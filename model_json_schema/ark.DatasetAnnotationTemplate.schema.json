  "required": [
    "datasetType",
    "assay",
    "diagnosis",
    "dataSubtype",
    "Component",
    "datasetDescription",
    "datasetStatus",
    "dataType",
    "acknowledgmentStatement",
    "biospecimenType",
    "project",
    "ARKRelease",
    "program"
  ],
  "allOf": [
    {
      "if": {
        "properties": {
          "program": {
            "enum": [
              "AMP RA/SLE"
            ]
          }
        },
        "required": [
          "program"
        ]
      },
      "then": {
        "properties": {
          "programPhase": {
            "type": "array",
            "items": {
              "enum": [
                "II",
                "I"
              ]
            },
            "maxItems": 2
          }
        },
        "required": [
          "programPhase"
        ]
      }
    }
  ]
