    "datasetDescription": {
      "not": {
        "type": "null"
      },
      "minLength": 1
    },
    "biospecimenSubtype": {
      "enum": [
        "cell or tissue lysate",
        "nuclei suspension",
        "frozen tissue",
        "FFPE tissue",
        "PFA-fixed tissue",
        "fresh tissue",
        "cell suspension",
        "supernatant",
        "flow-sorted cells",
        ""
      ]
