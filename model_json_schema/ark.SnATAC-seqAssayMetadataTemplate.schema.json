        "10x Cell Ranger Human GRCh38 2024-A"
      ]
    },
    "dataCollectionBatch": {},
    "sampleProcessingBatch": {},
    "inputCellCount": {
      "not": {
        "type": "null"
      },
      "minLength": 1
    },
    "percentCellViability": {
      "not": {
        "type": "null"
      },
      "minLength": 1
    },
    "assay": {
      "type": "array",
      "items": {
        "enum": [
