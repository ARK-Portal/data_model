        "Cell Ranger v7.2.0",
        "Cell Ranger v7.0.1",
        ""
      ]
    },
    "platform": {
      "type": "array",
      "items": {
        "enum": [
          "Helios Mass Cytometer",
          "BD FACSMelody",
          "Chromium Next GEM Chip M",
          "Sony MA900",
          "Illumina NovaSeq 6000",
          "BD LSRFortessa",
          "Chromium Xo",
