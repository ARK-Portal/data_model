    },
    "percentCellViability": {
      "not": {
        "type": "null"
      },
      "minLength": 1
    },
    "sampleProcessingBatch": {},
    "softwareAndVersion": {
      "enum": [
        "Cell Ranger v6.0.1",
        "Space Ranger 3.0.1",
        "Cell Ranger v3.0.2",
        "demuxlet",
        "Cell Ranger v6.1.2",
