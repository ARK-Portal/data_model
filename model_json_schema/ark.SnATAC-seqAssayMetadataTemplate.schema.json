        "Cell Ranger v3.1.0",
        "Cell Ranger v7.0.1",
        "Cell Ranger v3.0.1",
        "Cell Ranger v9.0.0",
        "Space Ranger 3.1.3",
        ""
      ]
    },
    "specimenModality": {
      "enum": [
        "single specimen",
        "multispecimen"
      ]
    },
    "dataCollectionBatch": {},
    "Component": {
      "not": {
        "type": "null"
      },
