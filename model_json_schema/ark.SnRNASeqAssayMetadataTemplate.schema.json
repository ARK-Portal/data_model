{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/ark",
  "title": "ark",
  "type": "object",
  "properties": {
    "percentCellViability": {
      "not": {
        "type": "null"
      },
      "minLength": 1
    },
    "nucleicAcidSource": {
      "type": "array",
